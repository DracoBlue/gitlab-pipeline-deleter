{
  "name": "gitlab-pipeline-deleter",
  "version": "0.1.0",
  "description": "A CLI tool that deletes old GitLab pipelines",
  "main": "target/src/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "keywords": [
    "gitlab",
    "pipeline",
    "ci",
    "clean",
    "delete",
    "remove"
  ],
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint src test --ext '.ts'",
    "copy:paste:detection": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/unit/**/*.test.*'",
    "test:unit:coverage": "c8 npm run test:unit"
  },
  "dependencies": {
    "got": "11.7.0",
    "ink": "3.0.6",
    "react": "16.13.1",
    "tslib": "2.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.6",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/chai": "4.2.12",
    "@types/mocha": "8.0.3",
    "@types/react": "16.9.49",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "c8": "7.3.1",
    "chai": "4.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "ink-testing-library": "2.0.1",
    "jscpd": "3.3.19",
    "mocha": "8.1.3",
    "prettier": "2.1.2",
    "semantic-release": "17.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  }
}
