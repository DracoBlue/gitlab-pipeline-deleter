    "c8": "7.3.1",
    "chai": "4.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "jscpd": "3.3.19",
    "mocha": "8.1.3",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
